{
  "extends": [
    // "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "align": [true,
      "parameters"
    ],
    "class-name": true,
    "comment-format": [true,
      "check-space",
      "check-lowercase"
    ],
    "curly": true,
    "eofline": true,
    "indent": [true, 2],
    "jsx-curly-spacing": [true,
      "always"
    ],
    "jsx-wrap-multiline": false,
    "max-line-length": [true, 150],
    "no-consecutive-blank-lines": [true],
    "no-console": [true,
      "log",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-require-imports": false,
    "no-shadowed-variable": true,
    "no-trailing-whitespace": true,
    "no-var-keyword": true,
    "no-var-requires": false,
    "object-curly-spacing": true,
    "one-line": [true,
      "check-else",
      "check-whitespace",
      "check-open-brace"
    ],
    "prefer-const": true,
    "quotemark": [true,
      "single",
      "avoid-escape",
      "jsx-single"
    ],
    "semicolon": [true, "never"],
    "ter-prefer-arrow-callback": true,
    "typedef-whitespace": [true, {
      "call-signature": "nospace",
      "index-signature": "nospace",
      "parameter": "nospace",
      "property-declaration": "nospace",
      "variable-declaration": "nospace"
    }],
    "use-isnan": true,
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
